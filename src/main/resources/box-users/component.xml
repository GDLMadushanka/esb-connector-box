<component name="box-users" type="synapse/template">
    <subComponents>
        <component name="listUsers">
            <file>listUsers.xml</file>
            <description>Returns a list of all users for the Enterprise along with their user_id,
                public_name, and login.
            </description>
        </component>
        <component name="moveFolder">
            <file>moveFolder.xml</file>
            <description>Moves all of the owned content from within one user&apos;s folder into a
                new folder in another user&apos;s account.
            </description>
        </component>
    </subComponents>
</component>